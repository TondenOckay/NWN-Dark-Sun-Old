/*
   This removes the subrace from the PC
*/
////////////////////////////////////////////////////////////////////////////////
// Required Includes
#include "gen_subs_inc"

void main()
{
 object oPC = GetLastUsedBy();
 if(!GetIsPC(oPC)) { return; }
 GSS_RemoveSubrace(oPC);
 SetLocalInt(oPC, "GSS_SUB_TAKE", FALSE);
}
